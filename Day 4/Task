1. Odd numbers in array
function(array){
  for(var i = 0 ; i< array.length ; i++){
    if(array[i]%2!=0){
      console.log(array[i])
    } 
  }
}


(function(array){
  for(var i = 0 ; i< array.length ; i++){
    if(array[i]%2!=0){
      console.log(array[i])
     } 
  }
 })([1,2,3,4])
             

